{
  "name": "seoul-moment-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build api",
    "build:api": "nest build api",
    "build:batch": "nest build batch",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start api",
    "start:api": "nest start api",
    "start:batch": "nest start batch",
    "start:local": "cross-env NODE_ENV=local nest start api --watch",
    "start:batch:local": "cross-env NODE_ENV=local nest start batch --watch",
    "start:dev": "cross-env NODE_ENV=development nest start api --watch",
    "start:batch:dev": "cross-env NODE_ENV=development nest start batch --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/apps/api/main",
    "start:batch:prod": "cross-env NODE_ENV=production node dist/apps/batch/main",
    "start:debug": "nest start api --debug --watch",
    "start:batch:debug": "nest start batch --debug --watch",
    "lint": "eslint \"{apps,libs}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=test jest --selectProjects unit",
    "test:watch": "cross-env NODE_ENV=test jest --watch --selectProjects unit",
    "test:cov": "cross-env NODE_ENV=test jest --coverage --selectProjects unit",
    "test:debug": "cross-env NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --selectProjects unit",
    "test:e2e": "cross-env NODE_ENV=test jest --config ./apps/api/test/jest-e2e.json",
    "test:integration": "cross-env NODE_ENV=test jest --selectProjects integration --runInBand",
    "test:integration:watch": "cross-env NODE_ENV=test jest --watch --selectProjects integration",
    "test:db:up": "docker-compose -f docker-compose.test.yml up -d",
    "test:db:down": "docker-compose -f docker-compose.test.yml down -v",
    "test:db:logs": "docker-compose -f docker-compose.test.yml logs -f test-db",
    "test:db:cleanup": "npm run test:db:down && sleep 2 && npm run test:db:up",
    "test:full": "npm run test:db:down && sleep 2 && npm run test:db:up && sleep 5 && npm run test:integration && npm run test:db:down",
    "prepare": "husky || true"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.888.0",
    "@aws-sdk/s3-request-presigner": "^3.888.0",
    "@liaoliaots/nestjs-redis": "^10.0.0",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "^8.1.1",
    "@nestjs/typeorm": "^11.0.0",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cross-env": "^10.0.0",
    "googleapis": "^148.0.0",
    "helmet": "^8.1.0",
    "moment": "^2.30.1",
    "moment-timezone": "^0.6.0",
    "morgan": "^1.10.1",
    "morgan-body": "^2.6.9",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "sharp": "^0.34.5",
    "typeorm": "^0.3.26",
    "typeorm-naming-strategies": "^4.1.0",
    "typeorm-transactional": "^0.5.0",
    "uuid": "^11.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-spellcheck": "^0.0.20",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.7",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/libs/",
      "<rootDir>/apps/",
      "<rootDir>/test/"
    ],
    "moduleNameMapper": {
      "^@app/database(|/.*)$": "<rootDir>/libs/database/src/$1",
      "^@app/config(|/.*)$": "<rootDir>/libs/config/src/$1",
      "^@app/repository(|/.*)$": "<rootDir>/libs/repository/src/$1",
      "^@app/common(|/.*)$": "<rootDir>/libs/common/src/$1",
      "^@app/external(|/.*)$": "<rootDir>/libs/external/src/$1",
      "^@app/http(|/.*)$": "<rootDir>/libs/http/src/$1",
      "^@app/cache(|/.*)$": "<rootDir>/libs/cache/src/$1",
      "^@app/auth(|/.*)$": "<rootDir>/libs/auth/src/$1"
    },
    "projects": [
      {
        "displayName": "unit",
        "testMatch": [
          "<rootDir>/(libs|apps)/**/*.spec.ts"
        ],
        "testEnvironment": "node",
        "moduleFileExtensions": [
          "js",
          "json",
          "ts"
        ],
        "rootDir": ".",
        "transform": {
          "^.+\\.(t|j)s$": "ts-jest"
        },
        "moduleNameMapper": {
          "^@app/database(|/.*)$": "<rootDir>/libs/database/src/$1",
          "^@app/config(|/.*)$": "<rootDir>/libs/config/src/$1",
          "^@app/repository(|/.*)$": "<rootDir>/libs/repository/src/$1",
          "^@app/common(|/.*)$": "<rootDir>/libs/common/src/$1",
          "^@app/external(|/.*)$": "<rootDir>/libs/external/src/$1",
          "^@app/http(|/.*)$": "<rootDir>/libs/http/src/$1",
          "^@app/cache(|/.*)$": "<rootDir>/libs/cache/src/$1"
        }
      },
      {
        "displayName": "integration",
        "testMatch": [
          "<rootDir>/test/**/*.spec.ts"
        ],
        "testEnvironment": "node",
        "setupFilesAfterEnv": [
          "<rootDir>/test/setup/jest.setup.ts"
        ],
        "moduleFileExtensions": [
          "js",
          "json",
          "ts"
        ],
        "rootDir": ".",
        "transform": {
          "^.+\\.(t|j)s$": "ts-jest"
        },
        "moduleNameMapper": {
          "^@app/database(|/.*)$": "<rootDir>/libs/database/src/$1",
          "^@app/config(|/.*)$": "<rootDir>/libs/config/src/$1",
          "^@app/repository(|/.*)$": "<rootDir>/libs/repository/src/$1",
          "^@app/common(|/.*)$": "<rootDir>/libs/common/src/$1",
          "^@app/external(|/.*)$": "<rootDir>/libs/external/src/$1",
          "^@app/http(|/.*)$": "<rootDir>/libs/http/src/$1",
          "^@app/cache(|/.*)$": "<rootDir>/libs/cache/src/$1"
        },
        "globals": {
          "ts-jest": {
            "tsconfig": "tsconfig.json"
          }
        }
      }
    ]
  },
  "optionalDependencies": {
    "@css-inline/css-inline-linux-x64-gnu": "^0.17.0"
  }
}
