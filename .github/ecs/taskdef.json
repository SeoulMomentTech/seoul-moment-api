{
  "family": "seoul-moment-dev",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "executionRoleArn": "arn:aws:iam::156321659479:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::156321659479:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "seoul-moment-api",
      "image": "156321659479.dkr.ecr.ap-northeast-2.amazonaws.com/seoul-moment-api-dev",
      "essential": true,
      "portMappings": [{ "containerPort": 3000, "protocol": "tcp" }],
      "secrets": [
        {
          "name": "NODE_ENV",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:156321659479:secret:seoul-moment-api-dev-lsglXS:NODE_ENV::"
        },
        {
          "name": "PORT",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:156321659479:secret:seoul-moment-api-dev-lsglXS:PORT::"
        },
        {
          "name": "API_VERSION",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:156321659479:secret:seoul-moment-api-dev-lsglXS:API_VERSION::"
        },
        {
          "name": "IMAGE_DOMAIN_NAME",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:156321659479:secret:seoul-moment-api-dev-lsglXS:IMAGE_DOMAIN_NAME::"
        },
        {
          "name": "DATABASE_HOST",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:156321659479:secret:seoul-moment-api-dev-lsglXS:DATABASE_HOST::"
        },
        {
          "name": "DATABASE_PORT",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:156321659479:secret:seoul-moment-api-dev-lsglXS:DATABASE_PORT::"
        },
        {
          "name": "DATABASE_USERNAME",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:156321659479:secret:seoul-moment-api-dev-lsglXS:DATABASE_USERNAME::"
        },
        {
          "name": "DATABASE_PASSWORD",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:156321659479:secret:seoul-moment-api-dev-lsglXS:DATABASE_PASSWORD::"
        },
        {
          "name": "DATABASE_NAME",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:156321659479:secret:seoul-moment-api-dev-lsglXS:DATABASE_NAME::"
        },
        {
          "name": "JWT_SECRET",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:156321659479:secret:seoul-moment-api-dev-lsglXS:JWT_SECRET::"
        },
        {
          "name": "JWT_EXPIRES_IN",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:156321659479:secret:seoul-moment-api-dev-lsglXS:JWT_EXPIRES_IN::"
        },
        {
          "name": "REDIS_HOST",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:156321659479:secret:seoul-moment-api-dev-lsglXS:REDIS_HOST::"
        },
        {
          "name": "REDIS_PORT",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:156321659479:secret:seoul-moment-api-dev-lsglXS:REDIS_PORT::"
        },
        {
          "name": "REDIS_DB",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:156321659479:secret:seoul-moment-api-dev-lsglXS:REDIS_DB::"
        },
        {
          "name": "GOOGLE_SHEET_SERVICE_EMAIL",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:156321659479:secret:seoul-moment-api-dev-lsglXS:GOOGLE_SHEET_SERVICE_EMAIL::"
        },
        {
          "name": "GOOGLE_SHEET_SERVICE_PRIMARY",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:156321659479:secret:seoul-moment-api-dev-lsglXS:GOOGLE_SHEET_SERVICE_PRIMARY::"
        },
        {
          "name": "SERPER_URL",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:156321659479:secret:seoul-moment-api-dev-lsglXS:SERPER_URL::"
        },
        {
          "name": "SERPER_API_KEY",
          "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:156321659479:secret:seoul-moment-api-dev-lsglXS:SERPER_API_KEY::"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/seoul-moment-dev",
          "awslogs-region": "ap-northeast-2",
          "awslogs-stream-prefix": "api"
        }
      }
    }
  ]
}
